---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ site.base }}/</loc>
        <lastmod>{{ site.time | date: "%FT%T.000Z" }}</lastmod>
    </url>
    {% for page in site.pages %}
    <url>
        <loc>{{ site.base }}/#!{{ page.url }}</loc>
        <lastmod>{{ site.time | date: "%FT%T.000Z" }}</lastmod>
    </url>
    {% endfor %}
    {% for page in site.pages %}
    <url>
        <loc>{{ site.base }}{{ page.url }}</loc>
        <lastmod>{{ site.time | date: "%FT%T.000Z" }}</lastmod>
    </url>
    {% endfor %}
</urlset>
